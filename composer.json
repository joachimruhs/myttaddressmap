{
  "name": "wsr/myttaddressmap",
  "description": "Google maps for tt_address with radial search and categories",
  "keywords": ["TYPO3", "Google maps", "tt_address", "extension", "radial search"],
  "homepage": "https://www.web-services-ruhs.de",
  "authors": [
    {
      "name": "Joachim Ruhs",
      "email": "postmaster@joachim-ruhs.de"
    }
  ],
  "license": "MIT",
  "type": "typo3-cms-extension",
  "require": {
	"typo3/cms-core": "^10.4 || >=11.1.1 || ^11.2",
	"typo3/cms-frontend": "^10.4 || >=11.1.1 || ^11.2"
  },
  "replace": {
    "wsr/myttaddressmap": "self.version",
    "typo3-ter/myttaddressmap": "self.version"
  },
  "autoload": {
    "psr-4": {
      "WSR\\Myttaddressmap\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
       "extension-key": "myttaddressmap"
    }
  }
  
}
